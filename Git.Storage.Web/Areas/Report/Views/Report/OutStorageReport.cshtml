@{
    Layout = null;
}

<!DOCTYPE html>
<!--[if IE 8]> <html lang="zh" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="zh" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="zh">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    @Html.Partial("_Title")
    @Html.Raw(ViewBag.CssFile)
</head>
<body class="fixed-top">
    @Html.Partial("_Top")
    <div id="container" class="row-fluid @ViewBag.MenuStatus">
        @Html.Partial("_LeftMenu")
        <div id="body">
            <div id="widget-config" class="modal hide">
                <div class="modal-header">
                    <button data-dismiss="modal" class="close" type="button"></button>
                    <h3>widget Settings</h3>
                </div>
                <div class="modal-body">
                    <p>Here will be a configuration form</p>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row-fluid">
                    <div class="span12">
                        @Html.Raw(ViewBag.NavMenu)
                    </div>
                </div>
                <div class="row-fluid">
                    <div class="span12">
                        <div class="tabbable tabbable-custom boxless">
                            <ul class="nav nav-tabs" id="liStatusGroup">
                                <li class="">
                                    <input type="hidden" value="10" /><a href="javascript:void(0)" data-toggle="tab">近10天</a></li>
                                <li class="active">
                                    <input type="hidden" value="30" /><a class="advance_form_with_chosen_element" href="javascript:void(0)" data-toggle="tab">近30天</a></li>
                                <li>
                                    <input type="hidden" value="60" /><a class="advance_form_with_chosen_element" href="javascript:void(0)" data-toggle="tab">近60天</a></li>
                                <li>
                                    <input type="hidden" value="36500" /><a class="advance_form_with_chosen_element" href="javascript:void(0)" data-toggle="tab">历史>></a></li>
                            </ul>
                            <div class="tab-content">
                                <div class="widget-body">
                                    <div class="clearfix margin-bottom-10">
                                        <div class="span6">
                                            <div class="widget">
                                                <div class="widget-title">
                                                    <h4><i class="icon-reorder"></i>出库报表</h4>
                                                </div>
                                                <div class="widget-body">
                                                    <table class="table table-striped table-bordered table-hover" id="tabInfoDetail">
                                                        <thead>                                                          
                                                          <tr>
                                                                <th style="width: 40px;">序号</th>
                                                                <th>时间</th>
                                                                <th class="hidden-480">数量</th>
                                                                <th class="hidden-480">金额</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                        </tbody>
                                                    </table>
                                                     <div class="row-fluid" id="mypagerDetail">
                                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="span6">
                                            <div class="widget">
                                                <div class="widget-title">
                                                    <h4><i class="icon-reorder"></i>出库报表</h4>
                                                    <ul class="nav nav-tabs">
                                                        <li style="float:right;">
                                                            <a class="btn"><i onclick="OutStorageReport.OutStorageAmpie(-1);" style="cursor: pointer;" class="icon-fast-backward"></i>
                                                                &nbsp;&nbsp;&nbsp;&nbsp; <i onclick="OutStorageReport.OutStorageAmpie(1);;" style="cursor: pointer;" class="icon-fast-forward"></i></a>
                                                            <input type="hidden" id="hdIndex" name="hdIndex" value="1" />
                                                          
                                                        </li>
                                                    </ul>
                                                </div>
                                                <div class="widget-body">
                                                    <div class="row-fluid" id="InStorageAmlineDIV">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="widget box light-grey">
                                    <div class="widget-body">
                                        <div class="clearfix margin-bottom-10">
                                            <table class="table table-striped table-bordered table-hover" id="tabInfo">
                                                <thead>
                                                    <tr class="widget-title">
                                                        <th class="hidden-480">出库单号</th>
                                                        <th class="hidden-480">日期</th>
                                                        <th class="hidden-480">供应商名称</th>
                                                        <th class="hidden-480">总数</th>
                                                        <th class="hidden-480">总价</th>
                                                        <th>明细</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                            <div class="row-fluid" id="mypager">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @Html.Partial("_Foot")
    <script type="text/javascript">
        $(document).ready(function () {
            OutStorageReport.PageClick(1, 10);
            OutStorageReport.PageClickDetail(1, 10);
            OutStorageReport.OutStorageAmpie(0);
            OutStorageReport.TabClick();
        });
    </script>
</body>

</html>
